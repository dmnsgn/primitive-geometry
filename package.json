{
  "name": "primitive-geometry",
  "version": "1.2.0",
  "description": "A package exporting geometries for 3D rendering, including normals, UVs and cell indices (faces).",
  "main": "index.js",
  "author": "Damien Seguin (https://dmnsgn.me)",
  "license": "MIT",
  "scripts": {
    "start": "budo demo/index.js:bundle.js --open --live",
    "build": "browserify demo/index.js | uglifyjs -c > bundle.js",
    "test": "npm run build"
  },
  "devDependencies": {
    "async-preloader": "^4.8.1",
    "browserify": "^16.5.0",
    "budo": "^11.6.3",
    "dat.gui": "^0.7.6",
    "geom-triangulate": "^1.0.1",
    "get-canvas-context": "^1.0.2",
    "gl-matrix": "^3.1.0",
    "orbit-controls": "^1.2.4",
    "perspective-camera": "^2.0.1",
    "regl": "^1.3.13",
    "uglify-es": "^3.3.9"
  },
  "dependencies": {
    "primitive-box": "^1.0.0",
    "primitive-capsule": "^3.0.1",
    "primitive-circle": "^1.0.2",
    "primitive-cube": "^2.0.1",
    "primitive-cylinder": "^1.0.3",
    "primitive-ellipsoid": "^1.1.0",
    "primitive-icosphere": "^1.0.2",
    "primitive-plane": "^2.0.0",
    "primitive-quad": "^2.0.0",
    "primitive-rounded-cube": "^1.0.2",
    "primitive-sphere": "^3.0.0",
    "primitive-torus": "^1.0.4"
  },
  "keywords": [
    "simplicial",
    "complex",
    "mesh",
    "geom",
    "geometry",
    "primitive",
    "stackgl",
    "glsl",
    "webgl",
    "gl"
